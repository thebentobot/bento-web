{
    "plugins": ["prettier-plugin-astro", "prettier-plugin-svelte"],
    "printWidth": 100,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        },
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        },
        {
            "files": ["*.ts"],
            "options": {
                "parser": "typescript"
            }
        }
    ]
}
